<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hotappproto.MenuActivity"
    tools:ignore="MergeRootFrame" >
    <FrameLayout
            android:layout_width="280dp"
            android:layout_height="405dp" android:layout_gravity="right|top" android:layout_marginRight="10dp"
            android:layout_marginTop="12dp" android:id="@+id/livingRoomFrame">
    </FrameLayout>
    <FrameLayout
            android:layout_width="285dp"
            android:layout_height="196dp" android:layout_gravity="left|top" android:id="@+id/kitchenFrame"
            android:layout_marginTop="12dp" android:layout_marginLeft="12dp">
    </FrameLayout>
</FrameLayout>
